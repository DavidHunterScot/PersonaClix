    <section class="packages section">
      <div class="container">
        <a name="packages"></a>
        <h2>Packages</h2>
            
        <div class="packages">
          {% for package in packages %}
          <div class="package">
            <div class="title">{{ package.title }}</div>
                        
            <div class="specs">
              {% for spec in package.specs %}
              <div class="spec"><b>{{ spec.value }}</b> {{ spec.name }}</div>
              {% endfor %}
            </div>
          
            <div class="order button"><a href="{{ client_url }}/store/vps/{{ package.store_url }}" target="_blank">Order</a></div>
            <div class="price"><b>{{ package.price.amount }}</b> per {{ package.price.frequency }}</div>
          </div>
          {% endfor %}
        </div>
        <p><b>Scroll down to see more features and information.</b></p>
      </div>
    </section>
    
